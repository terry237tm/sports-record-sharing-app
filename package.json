{
  "name": "taro-cloudbase-mcp",
  "version": "1.0.0",
  "private": true,
  "description": "Taro + React + TypeScript 项目，集成腾讯云 CloudBase MCP Tools",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@tarojs/components": "4.1.6",
    "@tarojs/helper": "4.1.6",
    "@tarojs/plugin-framework-react": "^4.1.6",
    "@tarojs/plugin-platform-alipay": "4.1.6",
    "@tarojs/plugin-platform-h5": "^4.1.6",
    "@tarojs/plugin-platform-jd": "4.1.6",
    "@tarojs/plugin-platform-qq": "4.1.6",
    "@tarojs/plugin-platform-swan": "4.1.6",
    "@tarojs/plugin-platform-tt": "4.1.6",
    "@tarojs/plugin-platform-weapp": "4.1.6",
    "@tarojs/react": "^4.1.6",
    "@tarojs/router": "4.1.6",
    "@tarojs/runtime": "4.1.6",
    "@tarojs/shared": "4.1.6",
    "@tarojs/taro": "4.1.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.1.3",
    "taro-ui": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "@cloudbase/cloudbase-mcp": "^1.8.42",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@tarojs/cli": "4.1.6",
    "@tarojs/webpack5-runner": "4.1.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.5",
    "@types/react": "^18.0.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-preset-taro": "4.1.6",
    "css-loader": "3.4.2",
    "eslint": "^8.12.0",
    "eslint-config-taro": "4.1.6",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "8.4.18",
    "style-loader": "1.3.0",
    "stylelint": "14.4.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.0",
    "webpack": "5.78.0"
  }
}